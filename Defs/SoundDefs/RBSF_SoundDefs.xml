<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Horn Ram -->
	
	<SoundDef>
		<defName>RBSF_HornRam</defName>
		<context>MapOnly</context>
		<priorityMode>PrioritizeNearest</priorityMode>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
