	<!-- Reclusive -->
		
	<GeneDef ParentName="GeneMoodBase">
		<defName>RBSF_Mood_Reclusive</defName>
		<label>reclusive</label>
		<description>The mood of these pawns depends on how big or small their faction is; the smaller it is, the happier they will be.</description>
		<iconPath>UI/GeneIcons/RBSF_Reclusive</iconPath>
		<displayOrderInCategory>40</displayOrderInCategory>
		<biostatCpx>1</biostatCpx>
		<biostatMet>1</biostatMet>
		<forcedTraits>
			<li>
				<def>Recluse</def>
			</li>
		</forcedTraits>
		<missingGeneRomanceChanceFactor>0.7</missingGeneRomanceChanceFactor>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>shy</symbol></li>
				<li><symbol>meek</symbol></li>
				<li><symbol>reclu</symbol></li>
				<li><symbol>blush</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>mure</symbol></li>
				<li><symbol>hide</symbol></li>
				<li><symbol>peek</symbol></li>
				<li><symbol>bash</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

	<!-- Horizontal Pupils -->
	
	<GeneDef ParentName="GeneEyeColor">
		<defName>RBSF_HorizontalPupils</defName>
		<label>horizontal pupils</label>
		<description>Carriers of this gene have horizontal, slit-shaped pupils. These widen the carrier's field of view, though it's harder to see what's near, above and below them.</description>
		<biostatCpx>1</biostatCpx>
		<biostatMet>0</biostatMet>
		<displayOrderInCategory>4</displayOrderInCategory>
		<iconPath>UI/GeneIcons/RBSF_HorizontalPupils</iconPath>
		<renderNodeProperties>
		  <li Class="PawnRenderNodeProperties_Eye">
			<texPath>Things/Pawn/Humanlike/Eyes/RBSF_HorizontalPupils/RBSF_HorizontalPupils</texPath>
			<anchorTag>RightEye</anchorTag>
			<rotDrawMode>Fresh, Rotting</rotDrawMode>
			<parentTagDef>Head</parentTagDef>
			<drawSize>0.07</drawSize>
			<side>Right</side>
			<drawData>
			  <defaultData>
				<layer>54</layer>
				<offset>(0, 0, -0.25)</offset>
			  </defaultData>
			</drawData>
		  </li>
		  <li Class="PawnRenderNodeProperties_Eye">
			<texPath>Things/Pawn/Humanlike/Eyes/RBSF_HorizontalPupils/RBSF_HorizontalPupils</texPath>
			<anchorTag>LeftEye</anchorTag>
			<rotDrawMode>Fresh, Rotting</rotDrawMode>
			<parentTagDef>Head</parentTagDef>
			<drawSize>0.07</drawSize>
			<side>Left</side>
			<drawData>
			  <defaultData>
				<layer>54</layer>
				<offset>(0, 0, -0.25)</offset>
				<flip>true</flip>
			  </defaultData>
			  <dataWest>
				<flip>false</flip>
			  </dataWest>
			</drawData>
		  </li>
		</renderNodeProperties>
		<statFactors>
			<ShootingAccuracyFactor_Long>1.004</ShootingAccuracyFactor_Long>
			<ShootingAccuracyFactor_Medium>1.002</ShootingAccuracyFactor_Medium>
			<ShootingAccuracyFactor_Short>0.992</ShootingAccuracyFactor_Short>
			<ShootingAccuracyFactor_Touch>0.985</ShootingAccuracyFactor_Touch>
			<MortarMissRadiusFactor>1.2</MortarMissRadiusFactor>
		</statFactors>
		<!-- Symbol -->
		<symbolPack>
			<prefixSymbols>
				<li><symbol>hori</symbol></li>
				<li><symbol>pano</symbol></li>
				<li><symbol>prey</symbol></li>
				<li><symbol>elon</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>gate</symbol></li>
				<li><symbol>zontal</symbol></li>
				<li><symbol>rama</symbol></li>
				<li><symbol>plane</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- Natural Connection -->

	<GeneDef>
		<defName>RBSF_NaturalConnection</defName>
		<geneClass>Roos_Faun_Xenotype.RBSF_NaturalConnection</geneClass>
		<label>natural connection</label>
		<description>Carriers of this gene are spiritually intertwined with nature. Plants around this pawn will have their growth rate increased. The rate of growth scales with the pawn's Nature Connection stat, which can be increased by equipping certain weapons and items of clothing.</description>
		<iconPath>UI/GeneIcons/RBSF_NaturalConnection</iconPath>
		<statFactors>
			<RBSF_NatureConnection>1.15</RBSF_NatureConnection>
		</statFactors>
		<descriptionHyperlinks>
			<StatDef>RBSF_NatureConnection</StatDef>
		</descriptionHyperlinks>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<displayCategory>Miscellaneous</displayCategory>
		<displayOrderInCategory>180</displayOrderInCategory>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>gaia</symbol></li>
				<li><symbol>fae</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>sprout</symbol></li>
				<li><symbol>yad</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
	
	<!-- Chokevine Grasp -->

	<GeneDef>
		<defName>RBSF_ChokevineGrasp</defName>
		<label>chokevine grip</label>
		<description>The user's connection with nature allows them to summon chokevines from the ground below, constricting and slowing an enemy in their grasp. The range of this ability scales with the pawn's Nature Connection stat, which can be increased by equipping certain weapons and items of clothing.</description>
		<iconPath>UI/GeneIcons/RBSF_ChokevineGrasp</iconPath>
		<descriptionHyperlinks>
			<StatDef>RBSF_NatureConnection</StatDef>
		</descriptionHyperlinks>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<displayCategory>Ability</displayCategory>
		<displayOrderInCategory>180</displayOrderInCategory>
		<abilities>
			<li>RBSF_AbilityChokevineGrasp</li>
		</abilities>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>choke</symbol></li>
				<li><symbol>thorn</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>vine</symbol></li>
				<li><symbol>grasp</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

</Defs>